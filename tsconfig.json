{
	"compilerOptions": {
		"target": "ES6",
		"module": "commonjs",
		"moduleResolution": "node",
		"outDir": "out",
		"noLib": true,
    "noImplicitAny": true,
		"sourceMap": true,
    "rootDir": "src",
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noImplicitReturns": true
	},
	"exclude": [
		"node_modules",
    "out"
	]
}
